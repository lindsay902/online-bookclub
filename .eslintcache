iv>\r\n\r\n            <div>\r\n                <p>\r\n                    <span style={{ fontWeight: \"500\", marginRight: \"4px\" }}>\r\n                        {username}\r\n                    </span>\r\n                    {caption}\r\n                </p>\r\n            </div>\r\n\r\n            {comments ? (\r\n                comments.map((comment) => (\r\n                <Comment username={comment.username} caption={comment.comment} />  \r\n                ))\r\n            ) : (\r\n                <div></div>\r\n            )}\r\n\r\n            {user ? <CommentInput comments={comments} id={id}/> : <div></div>}\r\n            \r\n\r\n        </div>\r\n    );\r\n}\r\n\r\n","C:\\Users\\Lindsay\\Desktop\\react-firebase\\bookclub\\src\\containers\\feed\\index.js",[],"C:\\Users\\Lindsay\\Desktop\\react-firebase\\bookclub\\src\\components\\comment\\index.js",[],"C:\\Users\\Lindsay\\Desktop\\react-firebase\\bookclub\\src\\components\\comment-input\\index.js",["109","110"],"import React, { useState, useContext } from 'react';\r\nimport './style.css';\r\nimport { UserContext } from \"../../context/user\";\r\nimport { db } from '../../firebase';\r\n\r\nexport default function CommentInput({ comments, id }) {\r\n\r\n    const [user, setUser] = useContext(UserContext).user;\r\n    const [comment, setComment] = useState(\"\");\r\n    const [commentArray, setCommentArray] = useState(comments ? comments : []);\r\n\r\n    const addComment = () => {\r\n        if (comment !== \"\") {\r\n        \r\n            commentArray.push({\r\n                comment: comment,\r\n                username: user.email.replace(\"@gmail.com\", \"\").toLowerCase(),\r\n            });\r\n\r\n            db.collection(\"posts\")\r\n                .doc(id)\r\n                .update({\r\n                comments: commentArray,\r\n            })\r\n            .then(function () {\r\n                setComment(\"\");\r\n                console.log(\"Comment added!\");\r\n            })\r\n   